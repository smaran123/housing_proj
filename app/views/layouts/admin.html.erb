<!DOCTYPE html>
<html>
  <head>
    <title>Admin</title>
    <%= stylesheet_link_tag    'application',media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
      <script type="text/javascript" src="/assets/jquery.mask.min.js"></script>
    <script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
    <script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
    <%= csrf_meta_tags %>
  </head>
  
  <body>
    <header>
      <div class="topbar">     
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"><span class="glyphicon glyphicon-align-justify"></span></button>
        </div>
        <div class="navbar-collapse collapse">              
          <ul id="navigation">
            <li>
              <%= link_to t('.new', :default => t("helpers.links.new")), new_property_path, :class => 'btn-login' %>
            </li>
            
            <li><%= link_to "Sign out", destroy_admin_session_path, :method => :delete, :class => 'btn-login' %></li>
          </ul>  
        </div>                  
        <div class="clearF"></div>
      </div> 
    </header>
    <%= yield %>
    <%= yield :scripts %> 
  </body>
</html>
